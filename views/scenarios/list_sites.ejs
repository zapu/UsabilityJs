<%-render("views/header.ejs", {PageTitle: "Scenarios"})%>
<%-render("views/nav.ejs")%>

<div class="content-div">
	<h2>Sites</h2>
	<p>
		<ul>
			<% sites.forEach(function(site) { %>
				<li>
					<a href="./scenarios/<%=site._id%>"><%=site.name%></a>
				</li>
			<% }) %>
		</ul>
	</p>
</div>

<div class="content-div">
	<h2>Manage</h2>
	<ul>
		<li><a href="/scenarios/new_site">Add Site</a></li>
	</ul>
</div>

<%-render("views/footer.ejs")%>